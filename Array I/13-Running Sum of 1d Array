1480. Running Sum of 1d Array
https://leetcode.com/problems/running-sum-of-1d-array/description/


//solution
class Solution {
public:
    vector<int> runningSum(vector<int>& nums) {
        int n=nums.size();
        vector<int>v;
        int sum=0;
        for(int i=0;i<n;i++){
          sum+=nums[i];
          v.push_back(sum);
        }
        return v;
    }
};
